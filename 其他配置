#移除磁盘阵列 不管用！！！
sed -i 's/CONFIG_PACKAGE_kmod-dm-raid=y/CONFIG_PACKAGE_kmod-dm-raid=n/' .config
sed -i 's/CONFIG_PACKAGE_kmod-lib-raid6=y/CONFIG_PACKAGE_kmod-lib-raid6=n/' .config
